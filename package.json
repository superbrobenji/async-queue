{
    "name": "@superbrobenji/conqueue",
    "version": "0.1.2",
    "description": "A simple queue for executing promises concurrently. You can set timeouts, max concurrency as well as retries",
    "repository": {
        "url": "https://github.com/superbrobenji/async-queue",
        "type": "git"
    },
    "home": "https://github.com/superbrobenji/async-queue",
    "bugs": "https://github.com/superbrobenji/async-queue",
    "main": "index.js",
    "type": "module",
    "files": [
        "./dist/index.js",
        "./dist/cjs.js",
        "./dist/index.js.map",
        "./dist/cjs.js.map"
    ],
    "scripts": {
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --collectCoverage",
        "jsdoc": "jsdoc -a all -c jsdoc.json -r -R README.md src/index.js -d docs/jsdoc && jsdoc2md --files src/index.js > docs/package.md",
        "build": "node build.js"
    },
    "keywords": [
        "queue",
        "timeout",
        "timeouts",
        "retry",
        "retries",
        "list",
        "promises",
        "promise",
        "async",
        "await",
        "throttle",
        "concurrency",
        "concurrent",
        "wait"
    ],
    "exports": {
        "import": "./dist/index.js",
        "require": "./dist/cjs.js"
    },
    "types": "./dist/index.d.ts",
    "author": "superbrobenji",
    "license": "MIT",
    "devDependencies": {
        "docdash": "^2.0.2",
        "esbuild": "^0.24.0",
        "esbuild-plugin-d.ts": "^1.3.1",
        "jest": "^29.7.0",
        "jsdoc": "^4.0.4",
        "jsdoc-to-markdown": "^9.0.5",
        "typescript": "^5.6.3"
    }
}
